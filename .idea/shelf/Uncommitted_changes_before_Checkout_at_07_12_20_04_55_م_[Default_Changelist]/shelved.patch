+
+        // Set title.
+        builder.setContentTitle(title);
+
+        // Set content text.
+        builder.setContentText(textContent);
+
+        // Set notification send time.
+        builder.setWhen(sendTime);
+
+        // If true then cancel the notification automatically.
+        builder.setAutoCancel(autoCancel);
+
+        return builder;
+    }
